{
    "[json]": {
        "lowlight-patterns.rules": [
            {
                "startRule": ".*\\$schema.*",
                "endRule": "}",
                "opacity": "max"
            }
        ]
    },
    "[go]": {
        "lowlight-patterns.rules": [
            {
                "rule": "err != nil",
                "opacity": "max"
            }
        ]
    },
    "lowlight-patterns.rules": [
        {
            "rule": "err != nil",
            "opacity": "mid"
        }
    ],
    "lowlight-patterns.opacity": "mid",
    "lowlight-patterns.midOpacity": 0.5,
    "lowlight-patterns.maxNumberOfLinesToScan": 1000
}
